<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="3.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                                  http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_3_0.xsd">
    <navigation-rule>
        <from-view-id>/login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Cliente</from-outcome>
            <to-view-id>/cliente.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Cajero</from-outcome>
            <to-view-id>/Cajero/cajero.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Gerente</from-outcome>
            <to-view-id>Gerente/Gerente.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/cliente.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cerrarsesion</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <!--Comienza Parte de Pablo-->
    <navigation-rule>
        <from-view-id>/Cliente/cliente.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Transferir</from-outcome>
            <to-view-id>/Cliente/Transferir.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Retirar</from-outcome>
            <to-view-id>/Cliente/Retirar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Depositar</from-outcome>
            <to-view-id>/Cliente/Depositar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cerrarsesion</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/Cliente/Depositar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Depositos</from-outcome>
            <to-view-id>/Cliente/Depositohecho.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Regresar</from-outcome>
            <to-view-id>/Cliente/cliente.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/Cliente/Transferir.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>transferea</from-outcome>
            <to-view-id>/Cliente/transferenciahecha.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Regresar</from-outcome>
            <to-view-id>/Cliente/cliente.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/Cliente/transferenciahecha.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Regresar</from-outcome>
            <to-view-id>/Cliente/cliente.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/Cliente/Depositohecho.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Regresar</from-outcome>
            <to-view-id>/Cliente/cliente.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/Cliente/Retirar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Retirorealizado</from-outcome>
            <to-view-id>/Cliente/Retirohecho.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Regresar</from-outcome>
            <to-view-id>/Cliente/cliente.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/Cliente/Retirohecho.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Regresar</from-outcome>
            <to-view-id>/Cliente/cliente.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Regresar</from-outcome>
            <to-view-id>/Cliente/cliente.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>



    <navigation-rule>
        <from-view-id>/cliente.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cerrarsesion</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <!--Finaliza Parte de Pablo-->

    <!--Comienza Parte de Samuel-->
    <navigation-rule>
        <from-view-id>/Cajero/cajero.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>agregarCliente</from-outcome>
            <to-view-id>/Cajero/agregarCliente.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>prestamos</from-outcome>
            <to-view-id>/Cajero/Prestamos.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cerrarsesion</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/Cajero/agregarCliente.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>clienteAgregado</from-outcome>
            <to-view-id>/Cajero/clienteAgregado.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>regresar</from-outcome>
            <to-view-id>/Cajero/cajero.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/Cajero/clienteAgregado.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>regresar</from-outcome>
            <to-view-id>/Cajero/cajero.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/Cajero/Prestamos.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>aprobar</from-outcome>
            <to-view-id>/Cajero/aprobarPrestamo.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>denegar</from-outcome>
            <to-view-id>/Cajero/denegarPrestamo.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>regresar</from-outcome>
            <to-view-id>/Cajero/cajero.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/Cajero/aprobarPrestamo.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>prestamoAprobado</from-outcome>
            <to-view-id>/Cajero/prestamoAprobado.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>regresar</from-outcome>
            <to-view-id>/Cajero/cajero.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/Cajero/prestamoAprobado.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>regresar</from-outcome>
            <to-view-id>/Cajero/cajero.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/Cajero/denegarPrestamo.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>regresar</from-outcome>
            <to-view-id>/Cajero/cajero.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <!--Finaliza Parte de Samuel-->

    <!--Comienza Parte de Erick-->

    <navigation-rule>
        <from-view-id>Gerente/Gerente.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cerrarsesion</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>/Gerente/Gerente.xhtml</from-outcome>
            <to-view-id>Gerente/Gerente.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/Gerente/Gerente.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>registrarEmpleados</from-outcome>
            <to-view-id>/Gerente/Empleados.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/Gerente/Empleados.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>regresar</from-outcome>
            <to-view-id>/Gerente/Gerente.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/Gerente/Gerente.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>asignarTareasCajeros</from-outcome>
            <to-view-id>/Gerente/Tareas.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/Gerente/Tareas.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>regresar</from-outcome>
            <to-view-id>/Gerente/Gerente.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <!--Finaliza Parte de Erick-->


</faces-config>
